using System;
using System.Threading.Tasks;

public class HelloWorld
{
    static async Task Main()
    {   await Task.Delay(2000);
        Console.WriteLine("aaaaa");
        await MethodMain();
        Console.WriteLine("bbbbb");
    }
    
    static async Task MethodMain()
    {   
        Console.WriteLine("ccccc");
    }
}
